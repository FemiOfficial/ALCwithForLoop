{
  "name": "mealbookingapp",
  "version": "1.0.0",
  "description": "meal booking app",
  "main": "index.js",
  "scripts": {
    "start": "npm run migrate-test_undo && npm run migrate && npm run seed && babel-node ./apis/index.js",
    "sequelize": "./node_modules/.bin/sequelize",
    "migrate": "npm run sequelize db:migrate",
    "migrate-test": "npm run sequelize db:migrate",
    "migrate-test_undo": "npm run sequelize db:migrate:undo:all",
    "seed": "npm run sequelize db:seed:all",
    "node_env": "set NODE_ENV=test echo $NODE_ENV",
    "seed-test": "npm run sequelize db:seed:all",
    "test": "npm run migrate-test_undo && npm run migrate-test && npm run seed-test && nyc --require @babel/register mocha ./apis/test/*.js --timeout 30000000  --exit",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls-coverage": "coveralls < lcov.info",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
    "coverage": "npm test && npm run generate-lcov && npm run coveralls-coverage npm run codeclimate-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FemiOfficial/ALCwithForLoop.git"
  },
  "keywords": [
    "meal",
    "chop"
  ],
